
pessoa = {
    'Nome' : 'Vitor Augusto', 
    'Sobrenome': 'Oliveira', 
    'Idade': '23', 
    'Altura':1.7, 
    'Enderecos': [
        {'rua': 'tal tal', 'numero': 123}, 
        {'rua': 'outra rua', 'numero': 321}, 
    ],
}

print(pessoa['nome']) 
print(pessoa['sobrenome']) 

print() 

for chave in pessoa: 
    print(chave, pessoa [chave])
